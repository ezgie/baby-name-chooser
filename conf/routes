# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index(page: Integer?=1, fl: java.util.List[String]?=java.util.Collections.emptyList(), ll: java.util.List[String]?=java.util.Collections.emptyList())
# GET     /names                      controllers.Application.names()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
